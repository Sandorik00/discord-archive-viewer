{
  "include": ["src"],
  "compilerOptions": {
    "outDir": "dist",
    "strict": true,
    "sourceMap": true,
    "module": "commonJS",
    "target": "es6",
    "lib": ["es6", "dom"],
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "esModuleInterop": true
  }
}
